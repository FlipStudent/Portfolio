def ncr_mod_p(n, r, p):
    if r > n:
        return 0
    
    # Create an array to store binomial coefficients
    C = [[0 for x in range(r+1)] for x in range(n+1)]
    
    # Calculate value of Binomial Coefficient in bottom up manner
    for i in range(n+1):
        for j in range(min(i, r)+1):
            if j == 0 or j == i:
                C[i][j] = 1
            else:
                C[i][j] = (C[i-1][j-1] + C[i-1][j]) % p
    
    return C[n][r]

