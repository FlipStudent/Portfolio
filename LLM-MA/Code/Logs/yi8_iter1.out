def find_last_occurrence(arr, num):
    left, right = 0, len(arr) - 1

    while left <= right:
        mid = left + (right - left) // 2

        if arr[mid] == num:
            if mid == len(arr) - 1 or arr[mid + 1] != num:
                return mid
            left = mid + 1
        elif arr[mid] < num:
            left = mid + 1
        else:
            right = mid - 1

