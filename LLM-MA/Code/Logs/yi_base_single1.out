    for i in range(rows):
        for j in range(cols):
            x_offset = j * (3/2 * size)
            if i % 2 == 1:
                x_offset += size
            hexagon_points = []
            for k in range(6):
                angle = 2 * math.pi * k / 6
                x = start_x + x_offset + size * math.cos(angle)
                y = start_y + i * size * math.sqrt(3) / 2 + size * math.sin(angle)
                hexagon_points.append((x, y))
