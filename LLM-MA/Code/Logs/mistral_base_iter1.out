    if not (isinstance(n, int) and isinstance(m, int) and n >= 0 and m >= 0):
        return None

    if n == m:
        return None

    if n < m:
        n, m = m, n

    if n < 1 or m < 1:
        return None

    return (m - 1) * factorial(n - 1) // factorial(n - m)


